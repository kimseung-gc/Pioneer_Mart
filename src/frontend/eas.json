{
  "cli": {
    "version": ">= 16.3.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "ENVFILE": ".env.development"
      }
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "env": {
        "ENVFILE": ".env.production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
